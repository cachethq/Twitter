{
    "name": "steffjenl/twitter",
    "description": "A Twitter handle parser for Laravel 7",
    "keywords": ["twitter", "parser", "markdown", "Cachet", "CachetHQ"],
    "license": "MIT",
    "authors": [
        {
            "name": "Cachet",
            "email": "support@cachethq.io"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^7.0",
        "illuminate/support": "^7.0",
        "league/commonmark": "^1.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^3.1",
        "graham-campbell/markdown": "^13.1",
        "graham-campbell/testbench": "^5.1",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "CachetHQ\\Twitter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CachetHQ\\Tests\\Twitter\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "CachetHQ\\Twitter\\TwitterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
